х и зеленых чаев до экзотических травяных и фруктовых настоек. Наш квалифицированный персонал с радостью поможет гостям сориентироваться в нашем ассортименте и подобрать идеальную чашку.\r\n                                </p>\r\n                            </div>\r\n                            <div>\r\n                                <img src=\"images/About/about.webp\" className={styles.imgAbout}></img>\r\n                            </div>\r\n                        </div>\r\n                    </Show>\r\n                </Container>\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default About;"],"mappings":";AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,OAAOC,MAAM,MAAM,qBAAqB;AACxC,OAAOC,SAAS,MAAM,oBAAoB;AAC1C,OAAOC,KAAK,MAAM,UAAU;AAC5B,OAAOC,IAAI,MAAM,eAAe;AAChC,OAAO,qBAAqB;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAE7B,MAAMC,KAAK,GAAGA,CAAA,KAAM;EAChB,oBACID,OAAA;IAAKE,SAAS,EAAEP,MAAM,CAACQ,IAAK;IAACC,EAAE,EAAE,OAAQ;IAAAC,QAAA,eACrCL,OAAA;MAAKE,SAAS,EAAEP,MAAM,CAACW,OAAQ;MAAAD,QAAA,eAC3BL,OAAA,CAACJ,SAAS;QAAAS,QAAA,eACNL,OAAA,CAACF,IAAI;UAAAO,QAAA,eACDL,OAAA;YAAKE,SAAS,EAAEP,MAAM,CAACY,KAAM;YAAAF,QAAA,gBACzBL,OAAA;cAAKE,SAAS,EAAEP,MAAM,CAACa,IAAK;cAAAH,QAAA,gBACxBL,OAAA;gBAAAK,QAAA,EAAI;cAAK;gBAAAI,QAAA,EAAAC,YAAA;gBAAAC,UAAA;gBAAAC,YAAA;cAAA,OAAI,CAAC,eACdZ,OAAA;gBAAAK,QAAA,EAAG;cAEH;gBAAAI,QAAA,EAAAC,YAAA;gBAAAC,UAAA;gBAAAC,YAAA;cAAA,OAAG,CAAC;YAAA;cAAAH,QAAA,EAAAC,YAAA;cAAAC,UAAA;cAAAC,YAAA;YAAA,OACH,CAAC,eACNZ,OAAA;cAAAK,QAAA,eACIL,OAAA;gBAAKa,GAAG,EAAC,yBAAyB;gBAACX,SAAS,EAAEP,MAAM,CAACmB;cAAS;gBAAAL,QAAA,EAAAC,YAAA;gBAAAC,UAAA;gBAAAC,YAAA;cAAA,OAAM;YAAC;cAAAH,QAAA,EAAAC,YAAA;cAAAC,UAAA;cAAAC,YAAA;YAAA,OACpE,CAAC;UAAA;YAAAH,QAAA,EAAAC,YAAA;YAAAC,UAAA;YAAAC,YAAA;UAAA,OACL;QAAC;UAAAH,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OACJ;MAAC;QAAAH,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OACA;IAAC;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OACX;EAAC;IAAAH,QAAA,EAAAC,YAAA;IAAAC,UAAA;IAAAC,YAAA;EAAA,OACL,CAAC;AAEd,CAAC;AAACG,EAAA,GAtBId,KAAK;AAwBX,eAAeA,KAAK;AA